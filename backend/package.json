{
  "name": "backend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20.6.0",
    "pnpm": ">=8.7.0"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "lint": "tsc",
    "start": "node dist",
    "format": "prettier --check --ignore-path ../.gitignore .",
    "format:fix": "prettier --write --ignore-path ../.gitignore .",
    "build": "esbuild `find src \\( -name '*.ts' \\)` --platform=node --outdir=dist --resolve-extensions=.js",
    "check:types": "tsc --noEmit",
    "pretest": "pnpm run build"
  },
  "dependencies": {
    "@apollo/server": "^4.9.5",
    "@as-integrations/fastify": "^2.1.1",
    "@fastify/cors": "^8.4.1",
    "@fastify/websocket": "^8.3.1",
    "@graphql-tools/merge": "^9.0.1",
    "@hocuspocus/extension-database": "^2.8.1",
    "@hocuspocus/server": "^2.8.1",
    "axios": "^1.6.0",
    "fastify": "^4.25.2",
    "fastify-graceful-shutdown": "^3.5.1",
    "graphql": "^16.8.1",
    "graphql-tag": "^2.12.6",
    "graphql-upload": "^16.0.2",
    "lodash-es": "^4.17.21",
    "minio": "^7.1.3",
    "module-alias": "^2.2.3",
    "nanoid": "^5.0.4",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.34.0",
    "sharp": "^0.32.6",
    "sinon-chai": "^3.7.0",
    "tsconfig-paths": "^4.2.0",
    "umzug": "^3.4.0",
    "uuid": "^9.0.1",
    "y-protocols": "^1.0.6",
    "yjs": "^13.6.10",
    "yn": "^5.0.0"
  },
  "devDependencies": {
    "@swc/core": "^1.3.102",
    "@swc/helpers": "^0.5.3",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/module-alias": "^2.0.4",
    "@types/node": "^20.10.6",
    "chai": "^4.3.10",
    "esbuild": "^0.19.11",
    "mocha": "^10.2.0",
    "prettier": "^3.1.1",
    "proxyquire": "^2.1.3",
    "sequelize-cli": "^6.6.2",
    "sequelize-test-helpers": "^1.4.3",
    "sinon": "^17.0.1",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.8",
    "utf-8-validate": "^6.0.3"
  }
}
